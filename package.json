{
  "name": "transform-case",
  "displayName": "Transform Case - XXC",
  "description": "",
  "version": "0.0.2",
  "publisher": "xxc",
  "license": "MIT",
  "engines": {
    "vscode": "^1.105.0"
  },
  "categories": [
    "Other"
  ],
  "type": "module",
  "activationEvents": [],
  "main": "./out/extension.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/X-X-X-X-X-X-X-X-X-X-X-X-X/vscode-transfrom-case.git"
  },
  "contributes": {
    "commands": [
      {
        "command": "transform-case.transform",
        "title": "Transform Case"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "transform-case.transform",
          "group": "1_modification",
          "when": "editorHasSelection"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "pnpm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "pnpm run compile && pnpm run lint",
    "lint": "eslint src",
    "test": "vscode-test",
    "package": "vsce package"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.105.0",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.36.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "change-case": "^5.4.4"
  }
}